<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="preProcessorToLower" class="school21.spring.contest.Processor.PreProcessorToLower">
    </bean>
    <bean id="preProcessorToUpper" class="school21.spring.contest.Processor.PreProcessorToUpperImpl">
    </bean>

    <bean id="rendererErr" class="school21.spring.contest.Render.RendererErrImpl">
        <constructor-arg ref="preProcessorToUpper"/>
    </bean>
    <bean id="rendererStandard" class="school21.spring.contest.Render.RendererStandardImpl">
        <constructor-arg ref="preProcessorToLower"/>
    </bean>

    <bean id="printerWithDateTime" class="school21.spring.contest.printer.PrinterWithDateTimeImpl">
        <constructor-arg ref="rendererStandard"/>
    </bean>
    <bean id="PrinterWithPrefixImpl" class="school21.spring.contest.printer.PrinterWithPrefixImpl">
        <constructor-arg ref="rendererStandard"/>
        <property name="pref" value="Prefix"/>
    </bean>

</beans>